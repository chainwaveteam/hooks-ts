{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist"]
    },
    "www#build": {
      "dependsOn": ["^build", "generate-docs"],
      "env": ["NEXT_PUBLIC_GA_MEASUREMENT_ID"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "types-check": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": [],
      "cache": false
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "generate-docs": {},
    "update-readme": {}
  },
  "globalDependencies": ["tsconfig.json"]
}
